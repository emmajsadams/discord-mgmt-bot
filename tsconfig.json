{
  "compilerOptions": {
    "incremental": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./dist",
    "sourceMap": true,
    "strict": false,
    "allowJs": true,
    "lib": ["ES2019"],
    "target": "ES2019",
    "tsBuildInfoFile": "./buildcache/front-end",
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["./src/**/*"]
}
